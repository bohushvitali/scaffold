{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "jsRules": {},
  "linterOptions": {
    "exclude": []
  },
  "rules": {
    "jsdoc-format": false,
    "member-access": false,
    "no-console": false,
    "no-empty": false,
    "no-unused-expression": false,
    "no-unused-variable": false,
    "noImplicitAny": false,
    "semicolon": [true, "always"]
  },
  "rulesDirectory": []
}
