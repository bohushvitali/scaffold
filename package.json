{
  "name": "scaffold",
  "version": "1.0.1",
  "repository": "https://github.com/bohushvitali/scaffold.git",
  "author": "Vitali Bohush <bohushvitali@gmail.com>",
  "license": "MIT",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "scripts": {
    "watch": "tsc -p . -w",
    "dev": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "commit": "git-cz",
    "release": "standard-version",
    "push": "git push --follow-tags origin master",
    "lint": "tslint -c tslint.json --fix './{apps,libs}/**/*.ts'",
    "test": "jest",
    "test.watch": "jest --watch",
    "e2e": "codeceptjs run",
    "e2e.steps": "codeceptjs gherkin:snippets",
    "e2e.watch": "nodemon --exec \"yarn run e2e.steps && yarn e2e\"",
    "compile": "tsc -p .",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-angular": "7.5.0",
    "@commitlint/config-conventional": "7.5.0",
    "@commitlint/prompt": "7.5.0",
    "@types/jest": "24.0.5",
    "@webpack-cli/init": "0.1.3",
    "babel-loader": "8.0.5",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "codeceptjs": "2.0.4",
    "commitizen": "3.0.5",
    "css-loader": "2.1.0",
    "cz-conventional-changelog": "2.1.0",
    "gts": "0.9.0",
    "jest": "24.1.0",
    "jest-cucumber": "2.0.5",
    "node-sass": "4.11.0",
    "nodemon": "1.18.10",
    "puppeteer": "1.12.2",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "standard-version": "4.4.0",
    "style-loader": "0.23.1",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.3",
    "typescript": "3.3.3",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack": "4.29.3",
    "webpack-cli": "3.2.3"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  }
}